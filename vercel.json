{
  "buildCommand": "pnpm install --no-frozen-lockfile && pnpm run build",
  "builds": [
    {
      "src": "uv.handler.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/uv.handler.js", "dest": "/uv.handler.js" },
    { "src": "/service-worker.js", "dest": "/uv.sw.js" },
    { "src": "/(.*)", "dest": "/uv.handler.js" }
  ]
}
